{
  "manifest_version": 2,
  "name": "WhatDom",
  "version": "1.0",
  "description": "WhatDom example.",
  "browser_action": {
    "default_icon": "icon48.png",
    "default_title": "WhatDom",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab","scripting","tabs"],
  "content_scripts": [
    {
      "matches":   ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
